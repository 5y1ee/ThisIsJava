package ch05;

import java.util.Arrays;

public class ReferenceType {

	public static void main(String[] args) {
		
		int[] a1 = new int[] {1,2,3};
		int[] b1 = a1;
		
		System.out.println(Arrays.toString(b1));
		a1[0] = 99;
		System.out.println(Arrays.toString(b1));
		
		System.out.println(a1 == b1);
		
	}

}
