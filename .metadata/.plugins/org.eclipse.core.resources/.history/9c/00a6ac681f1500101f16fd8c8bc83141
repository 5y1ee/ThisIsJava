package codingtest;

import java.util.Arrays;

public class MembershipVIP {

	public static void main(String[] args) {
		
		int[] periods = {8, 23, 24};
		int[][] payments = 
				{{100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000}, 
				 {100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000, 100000}, 
				 {350000, 50000, 50000, 50000, 50000, 50000, 50000, 50000, 50000, 50000, 50000, 50000}};
		int[] estimates = {100000, 100000, 100000};
		
		int[] answer = solution(periods, payments, estimates);
		System.out.println(Arrays.toString(answer));
	}
	
	public static int[] solution(int[] periods, int[][] payments, int[] estimates) {
		int[] answer = {0,0};
		
		for (int i=0; i<payments.length; ++i)
			System.out.println(payments[i].length);
		
		// 2년 이상 5년 미만 : 90만원 이상
		// 5년 이상 : 60만원 이상
		// 기간이 2년 미만은 x
		
		int cnt = periods.length;
		
		for (int i=0; i<cnt; ++i) {
			int nextMonthPeriod = periods[i] + 1;
			int vipCut = 0;
			if (nextMonthPeriod<24) {	// 다음달도 2년 미만
				continue;
			}
			else if (nextMonthPeriod<60) {
				vipCut = 900000;
			}
			else if (nextMonthPeriod>=60) {
				vipCut = 600000;
			}
			
			int sum=estimates[i];
			for (int k=1; k<12; ++k) {
				sum+=payments[i][k];
			}
			
			System.out.println(sum);
			
			
			
		}
		
		
		
		return answer;
	}

}

/*
vip가 될 고객
아니게 될 고객
알림



