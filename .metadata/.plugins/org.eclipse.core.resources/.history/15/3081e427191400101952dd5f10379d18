package ch07.sec02;

public class Phone {
	public String model;
	public String color;
	
	public void bell()
	{
		System.out.println("bell");
	}
	
	public void sendVoice()
	{
		System.out.println("hellow");
	}
	
	public void receiveVoice()
	{
		System.out.println("hi");
	}
	
	public void hangUp()
	{
		System.out.println("bye");
	}
	
	
}
